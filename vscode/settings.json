{
    "C_Cpp.intelliSenseEngine": "Default",
    "editor.autoIndent": true,
    "editor.cursorBlinking": "solid",
    "editor.fontFamily": "Monaco, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.renderWhitespace": "none",
    "editor.rulers": [80, 120],
    "editor.snippetSuggestions": "top",
    "editor.wordWrap": "off",
    "emmet.includeLanguages": {
        "typescript": "javascriptreact",
        "typescriptreact": "javascriptreact",
        "php": "html"
    },
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "eslint.nodePath": "/home/dsifford/.yarn-global/bin",
    "extensions.autoUpdate": false,
    "keyboard.dispatch": "keyCode",
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "es5",
    "shellcheck.customArgs": ["-x"],
    "todohighlight.isEnable": true,
    "todohighlight.keywords": [
        {
            "backgroundColor": "#50fa7b",
            "color": "black",
            "text": "TODO:"
        },
        {
            "backgroundColor": "#ff5555",
            "color": "#f8f8f2",
            "text": "FIXME:"
        },
        {
            "backgroundColor": "#8be9fd",
            "color": "black",
            "text": "NOTE:"
        },
        {
            "backgroundColor": "#ffb86c",
            "color": "black",
            "text": "HACK:"
        }
    ],
    "uncrustify.configPath": "/home/dsifford/.dotfiles/uncrustify.cfg",
    "vim.enableNeovim": true,
    "vim.searchHighlightColor": "rgba(80, 250, 123, 0.6)",
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "window.menuBarVisibility": "toggle",
    "window.zoomLevel": 0,
    "workbench.colorCustomizations": {
        "editorBracketMatch.background": "#FF79C680",
        "editorBracketMatch.border": "#ff00ff"
    },
    "workbench.colorTheme": "Dracula",
    "workbench.startupEditor": "none",
    "codemetrics.basics.DiagnosticsEnabled": false,
    "codemetrics.nodeconfiguration.ReturnStatement": 0,
    "codemetrics.nodeconfiguration.CaseClause": 0,
    "codemetrics.basics.ComplexityTemplate": "Complexity is {0}",
    "codemetrics.nodeconfiguration.ContinueStatement": 0,
    "codemetrics.basics.DecorationModeEnabled": false,
    "emmet.showSuggestionsAsSnippets": true,
    "vim.foldfix": true,
    "vim.leader": "<space>",
    "vim.otherModesKeyBindingsNonRecursive": [
        {
            "after": [],
            "before": ["g", "r"],
            "commands": [
                {
                    "command": "editor.action.referenceSearch.trigger"
                }
            ]
        },
        {
            "after": [],
            "before": ["<leader>", "c"],
            "commands": [
                {
                    "command": "cursorHome"
                },
                {
                    "args": {
                        "snippet": "// "
                    },
                    "command": "editor.action.insertSnippet"
                }
            ]
        },
        {
            "after": [],
            "before": ["<leader>", "l"],
            "commands": [
                {
                    "command": "settings.cycle",
                    "args": {
                        "id": "codelens",
                        "overrideWorkspaceSettings": true,
                        "values": [
                            {
                                "editor.codeLens": true
                            },
                            {
                                "editor.codeLens": false
                            }
                        ]
                    }
                }
            ]
        },
        {
            "after": [],
            "before": ["<leader>", "f"],
            "commands": [
                {
                    "command": "editor.action.quickFix"
                }
            ] 
        },
        {
            "after": [],
            "before": ["<leader>", "b", "l"],
            "commands": [
                {
                    "command": "gitlens.toggleLineBlame"
                }
            ]
        },
        {
            "after": [],
            "before": ["<leader>", "b", "f"],
            "commands": [
                {
                    "command": "gitlens.toggleFileBlame"
                }
            ]
        },
        {
            "after": [],
            "before": ["<C-w>", ">"],
            "commands": [
                {
                    "command": "workbench.action.increaseViewSize"
                }
            ]
        },
        {
            "after": [],
            "before": ["<C-w>", "<"],
            "commands": [
                {
                    "command": "workbench.action.decreaseViewSize"
                }
            ]
        },
        {
            "after": [],
            "before": ["<C-w>", "="],
            "commands": [
                {
                    "command": "workbench.action.evenEditorWidths"
                }
            ]
        }

    ],
    "editor.codeLens": false,
    "projectManager.treeview.visible": false,
    "docker.showExplorer": false,
    "typescript.tsdk": "/home/dsifford/.yarn-global/lib/node_modules/typescript/lib",
    "explorer.decorations.badges": false,
    "python.formatting.provider": "yapf",
    "python.linting.flake8Enabled": true,
    "python.formatting.yapfArgs": [
        "--style",
        "google"
    ],
    "python.linting.flake8Args": [
        "--max-line-length", "100"
    ],
    "python.sortImports.args": [
        "-ac"
    ],
    "macros": {
        "openFile": [
            "list.select",
            "workbench.action.toggleSidebarVisibility"
        ]
    },
    "vim.easymotion": true,
    "vim.easymotionMarkerFontFamily": "Monaco",
    "vim.easymotionMarkerHeight": 20,
    "vim.easymotionMarkerWidthPerChar": 10,
    "vim.easymotionMarkerBackgroundColor": "#ff5555",
    "vim.easymotionMarkerFontWeight": "bold",
    "codemetrics.basics.OverviewRulerModeEnabled": false,
    "scss.validate": false,
    "css.validate": false,
    "stylelint.enable": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressUpdateNotice": true,
        "suppressWelcomeNotice": true
    },
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": true
    },
    "typescript.autoImportSuggestions.enabled": false,
}
