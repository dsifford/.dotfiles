{
    "color-highlight.languages": [
        "javascript",
        "typescript",
        "scss",
        "css",
        "sass",
        "less",
        "php",
        "xml",
        "html",
        "json",
        "yaml"
    ],
    "color-highlight.matchWords": false,
    "editor.cursorBlinking": "solid",
    "editor.fontFamily": "Monaco, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.minimap.enabled": true,
    "editor.renderIndentGuides": true,
    "editor.snippetSuggestions": "top",
    "editor.wordWrap": "off",
    "eslint.nodePath": "/home/dsifford/.yarn-global/bin",
    "rust.cargoHomePath": "/Users/dsifford/.cargo/bin/cargo",
    "shellcheck.customArgs": [
        "-x"
    ],
    "todohighlight.isEnable": true,
    "typescript.check.tscVersion": false,
    "uncrustify.configPath": "$HOME/.dotfiles/uncrustify.cfg",
    "vim.useSystemClipboard": true,
    "window.zoomLevel": 0,
    "workbench.iconTheme": "vs-seti",
    "workbench.welcome.enabled": false,
    "projectManager.openInNewWindow": false,
    "keyboard.dispatch": "keyCode",
    "window.menuBarVisibility": "toggle",
    "[cpp]": {
        "editor.quickSuggestions": false
    },
    "[c]": {
        "editor.quickSuggestions": false
    },
    "workbench.colorTheme": "Dracula",
    "color-highlight.markerType": "dot-before"
