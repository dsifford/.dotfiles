{
    "C_Cpp.intelliSenseEngine": "Default",
    "codemetrics.basics.ComplexityTemplate": "Complexity is {0}",
    "codemetrics.basics.DecorationModeEnabled": false,
    "codemetrics.basics.DiagnosticsEnabled": false,
    "codemetrics.basics.OverviewRulerModeEnabled": false,
    "codemetrics.nodeconfiguration.CaseClause": 0,
    "codemetrics.nodeconfiguration.ContinueStatement": 0,
    "codemetrics.nodeconfiguration.ReturnStatement": 0,
    "css.validate": false,
    "docker.showExplorer": false,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.autoIndent": true,
    "editor.codeLens": false,
    "editor.cursorBlinking": "solid",
    "editor.fontFamily": "Monaco, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.quickSuggestions": {
        "comments": true,
        "other": true,
        "strings": true
    },
    "editor.renderWhitespace": "none",
    "editor.rulers": [80, 120],
    "editor.snippetSuggestions": "top",
    "editor.wordWrap": "off",
    "emmet.includeLanguages": {
        "php": "html",
        "typescript": "javascriptreact",
        "typescriptreact": "javascriptreact"
    },
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "emmet.showSuggestionsAsSnippets": true,
    "explorer.decorations.badges": false,
    "extensions.autoUpdate": false,
    "files.exclude": {
        "**/*.pyo": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/CVS": true,
        "**/__pycache__": true,
        "**/node_modules": true,
    },
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": true,
        "suppressUpdateNotice": true,
        "suppressWelcomeNotice": true
    },
    "keyboard.dispatch": "keyCode",
    "macros": {
        "openFile": ["list.select", "workbench.action.toggleSidebarVisibility"]
    },
    "prettier.proseWrap": "always",
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "es5",
    "projectManager.treeview.visible": false,
    "python.formatting.provider": "yapf",
    "python.formatting.yapfArgs": ["--style", "google"],
    "python.linting.flake8Args": ["--max-line-length", "100"],
    "python.linting.flake8Enabled": true,
    "python.sortImports.args": ["-ac"],
    "scss.validate": false,
    "shellcheck.customArgs": ["-x"],
    "shellformat.flag": "-ci -bn",
    "stylelint.enable": true,
    "todohighlight.isEnable": true,
    "todohighlight.keywords": [
        {
            "backgroundColor": "#50fa7b",
            "color": "black",
            "text": "TODO:"
        },
        {
            "backgroundColor": "#ff5555",
            "color": "#f8f8f2",
            "text": "FIXME:"
        },
        {
            "backgroundColor": "#8be9fd",
            "color": "black",
            "text": "NOTE:"
        },
        {
            "backgroundColor": "#ffb86c",
            "color": "black",
            "text": "HACK:"
        }
    ],
    "typescript.autoImportSuggestions.enabled": false,
    "typescript.tsdk":
        "/home/dsifford/.yarn-global/lib/node_modules/typescript/lib",
    "uncrustify.configPath": "/home/dsifford/.uncrustify.cfg",
    "vim.easymotion": true,
    "vim.easymotionMarkerBackgroundColor": "#ff5555",
    "vim.easymotionMarkerFontFamily": "Monaco",
    "vim.easymotionMarkerFontWeight": "bold",
    "vim.easymotionMarkerHeight": 20,
    "vim.easymotionMarkerWidthPerChar": 10,
    "vim.enableNeovim": true,
    "vim.foldfix": true,
    "vim.leader": "<space>",
    "vim.otherModesKeyBindingsNonRecursive": [
        {
            "after": [],
            "before": ["g", "r"],
            "commands": [
                {
                    "command": "editor.action.referenceSearch.trigger"
                }
            ]
        },
        {
            "after": [],
            "before": ["<leader>", "c"],
            "commands": [
                {
                    "command": "cursorHome"
                },
                {
                    "args": {
                        "snippet": "// "
                    },
                    "command": "editor.action.insertSnippet"
                }
            ]
        },
        {
            "after": [],
            "before": ["<leader>", "l"],
            "commands": [
                {
                    "args": {
                        "id": "codelens",
                        "overrideWorkspaceSettings": true,
                        "values": [
                            {
                                "editor.codeLens": true
                            },
                            {
                                "editor.codeLens": false
                            }
                        ]
                    },
                    "command": "settings.cycle"
                }
            ]
        },
        {
            "after": [],
            "before": ["<leader>", "f"],
            "commands": [
                {
                    "command": "editor.action.quickFix"
                }
            ]
        },
        {
            "after": [],
            "before": ["<leader>", "b", "l"],
            "commands": [
                {
                    "command": "gitlens.toggleLineBlame"
                }
            ]
        },
        {
            "after": [],
            "before": ["<leader>", "b", "f"],
            "commands": [
                {
                    "command": "gitlens.toggleFileBlame"
                }
            ]
        },
        {
            "after": [],
            "before": ["<C-w>", ">"],
            "commands": [
                {
                    "command": "workbench.action.increaseViewSize"
                }
            ]
        },
        {
            "after": [],
            "before": ["<C-w>", "<"],
            "commands": [
                {
                    "command": "workbench.action.decreaseViewSize"
                }
            ]
        },
        {
            "after": [],
            "before": ["<C-w>", "="],
            "commands": [
                {
                    "command": "workbench.action.evenEditorWidths"
                }
            ]
        },
        {
            "after": [],
            "before": ["]", "c"],
            "commands": [
                {
                    "command": "workbench.action.editor.nextChange"
                }
            ]
        },
        {
            "after": [],
            "before": ["[", "c"],
            "commands": [
                {
                    "command": "workbench.action.editor.previousChange"
                }
            ]
        }
    ],
    "vim.searchHighlightColor": "rgba(80, 250, 123, 0.6)",
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "window.menuBarVisibility": "toggle",
    "window.zoomLevel": 0,
    "workbench.colorCustomizations": {
        "editorBracketMatch.background": "#FF79C680",
        "editorBracketMatch.border": "#ff00ff"
    },
    "workbench.colorTheme": "Dracula",
    "workbench.startupEditor": "none",
    "gitlens.keymap": "alternate",
    "gitlens.gitExplorer.files.layout": "list",
    "gitlens.codeLens.recentChange.command": "gitlens.diffWithPrevious",
    "gitlens.codeLens.scopes": [
        "document",
        "containers",
        "blocks"
    ],
    "gitlens.blame.highlight.locations": [
        "line",
        "overview",
        "gutter"
    ],
    "gitlens.hovers.currentLine.over": "line",
    "vim.sneak": true
}
