{
    "C_Cpp.intelliSenseEngine": "Default",
    "editor.autoIndent": true,
    "editor.cursorBlinking": "solid",
    "editor.fontFamily": "Monaco, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.renderWhitespace": "none",
    "editor.rulers": [
        80,
        120
    ],
    "editor.snippetSuggestions": "top",
    "editor.wordWrap": "off",
    "emmet.includeLanguages": {
        "typescript": "javascriptreact",
        "typescriptreact": "javascriptreact"
    },
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "eslint.nodePath": "/Users/dsifford/.yarn-global/bin",
    "extensions.autoUpdate": false,
    "keyboard.dispatch": "keyCode",
    "prettier.printWidth": 100,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "es5",
    "shellcheck.customArgs": [
        "-x"
    ],
    "todohighlight.isEnable": true,
    "todohighlight.keywords": [
        {
            "backgroundColor": "#50fa7b",
            "color": "black",
            "text": "TODO:"
        },
        {
            "backgroundColor": "#ff5555",
            "color": "#f8f8f2",
            "text": "FIXME:"
        },
        {
            "backgroundColor": "#8be9fd",
            "color": "black",
            "text": "NOTE:"
        },
        {
            "backgroundColor": "#ffb86c",
            "color": "black",
            "text": "HACK:"
        }
    ],
    "uncrustify.configPath": "/Users/dsifford/.dotfiles/uncrustify.cfg",
    "vim.disableAnnoyingNeovimMessage": true,
    "vim.enableNeovim": true,
    "vim.otherModesKeyBindingsNonRecursive": [
        {
            "after": [],
            "before": [
                "g",
                "r"
            ],
            "commands": [
                {
                    "command": "editor.action.referenceSearch.trigger"
                }
            ]
        },
        {
            "after": [],
            "before": [
                "<leader>",
                "c"
            ],
            "commands": [
                {
                    "command": "cursorHome"
                },
                {
                    "args": {
                        "snippet": "// "
                    },
                    "command": "editor.action.insertSnippet"
                }
            ]
        }
    ],
    "vim.searchHighlightColor": "rgba(80, 250, 123, 0.6)",
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "window.menuBarVisibility": "toggle",
    "window.zoomLevel": 0,
    "workbench.colorCustomizations": {
        "editorBracketMatch.background": "#FF79C680",
        "editorBracketMatch.border": "#ff00ff"
    },
    "workbench.colorTheme": "Dracula",
    "workbench.startupEditor": "none",
    "codemetrics.basics.CodeLensEnabled": false,
    "codemetrics.basics.DiagnosticsEnabled": false,
    "codemetrics.nodeconfiguration.ReturnStatement": 0,
    "codemetrics.nodeconfiguration.CaseClause": 0,
    "codemetrics.basics.ComplexityTemplate": "Complexity is {0}",
    "codemetrics.nodeconfiguration.ContinueStatement": 0,
    "vim.textwidth": 100,
    "emmet.showSuggestionsAsSnippets": true,
    "vim.foldfix": true
}
