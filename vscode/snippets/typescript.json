{
    "Console log": {
        "prefix": "cl",
        "body": [
            "console.log($1);$0"
        ],
        "description": "console.log()"
    },
    "Single line jsdoc comment": {
        "prefix": "cm",
        "body": [
            "/** $1 */$0"
        ],
        "description": "Insert a single line doc comment"
    },
    "Describe Block": {
        "prefix": "desc",
        "body": [
            "describe('$1', () => {",
            "\t$0",
            "});"
        ],
        "description": "Describe block for tests."
    },
    "Test Block": {
        "prefix": "test",
        "body": [
            "test('$1', ${2:async }() => {",
            "\t$0",
            "});"
        ]
    }
}
