{
	"C_Cpp.intelliSenseEngine": "Default",
	"breadcrumbs.enabled": true,
	"css.validate": false,
	"diffEditor.ignoreTrimWhitespace": true,
	"docker.showExplorer": false,
	"editor.acceptSuggestionOnEnter": "smart",
	"editor.autoIndent": true,
	"editor.cursorBlinking": "solid",
	"editor.fontFamily": "Monaco, 'Courier New', monospace",
	"editor.fontSize": 18,
	"editor.gotoLocation.multiple": "goto",
	"editor.quickSuggestions": {
		"comments": true,
		"other": true,
		"strings": true
	},
	"editor.renderWhitespace": "none",
	"editor.rulers": [
		100,
		120,
		80
	],
	"editor.suggest.localityBonus": true,
	"editor.suggestOnTriggerCharacters": false,
	"editor.suggestSelection": "first",
	"editor.wordWrap": "off",
	"emmet.includeLanguages": {
		"php": "html",
		"typescript": "javascriptreact",
		"typescriptreact": "javascriptreact"
	},
	"emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
	"emmet.showSuggestionsAsSnippets": true,
	"explorer.decorations.badges": true,
	"extensions.autoUpdate": false,
	"files.exclude": {
		"**/*.pyo": true,
		"**/.DS_Store": true,
		"**/.git": true,
		"**/.hg": true,
		"**/.svn": true,
		"**/__pycache__": true,
		"**/CVS": true,
		"**/node_modules": true
	},
	"gitlens.advanced.messages": {
		"suppressCommitHasNoPreviousCommitWarning": false,
		"suppressCommitNotFoundWarning": false,
		"suppressFileNotUnderSourceControlWarning": false,
		"suppressGitVersionWarning": false,
		"suppressLineUncommittedWarning": false,
		"suppressNoRepositoryWarning": false,
		"suppressResultsExplorerNotice": true,
		"suppressShowKeyBindingsNotice": true,
		"suppressUpdateNotice": true,
		"suppressWelcomeNotice": true
	},
	"gitlens.blame.highlight.locations": [
		"gutter",
		"line",
		"overview"
	],
	"gitlens.codeLens.enabled": false,
	"gitlens.codeLens.recentChange.command": "gitlens.diffWithPrevious",
	"gitlens.codeLens.scopes": [
		"blocks",
		"containers",
		"document"
	],
	"gitlens.hovers.annotations.over": "annotation",
	"gitlens.keymap": "none",
	"gitlens.views.fileHistory.enabled": true,
	"gitlens.views.lineHistory.enabled": true,
	"gitlens.views.repositories.files.layout": "list",
	"javascript.suggest.autoImports": false,
	"keyboard.dispatch": "keyCode",
	"merge-conflict.autoNavigateNextConflict.enabled": true,
	"phpcbf.standard": "dsifford",
	"prettier.proseWrap": "always",
	"prettier.singleQuote": true,
	"prettier.tabWidth": 4,
	"prettier.trailingComma": "es5",
	"python.formatting.provider": "black",
	"python.jediEnabled": false,
	"python.linting.mypyEnabled": true,
	"python.linting.mypyPath": "/usr/bin/mypy",
	"python.linting.pylintPath": "/home/dsifford/.local/bin/pylint",
	"python.pythonPath": "python3.7",
	"python.sortImports.args": [
		"-ac"
	],
	"references.preferredLocation": "view",
	"rust-client.updateOnStartup": true,
	"rust.clippy_preference": "on",
	"rust.goto_def_racer_fallback": true,
	"rust.unstable_features": true,
	"scss.validate": false,
	"search.usePCRE2": true,
	"shellcheck.customArgs": [
		"-x"
	],
	"stylelint.enable": true,
	"telemetry.enableTelemetry": false,
	"todo-tree.customHighlight": {
		"FIXME": {
			"background": "#ff5555",
			"foreground": "#f8f8f2",
			"icon": "alert",
			"iconColour": "#ff5555"
		}
	},
	"todo-tree.defaultHighlight": {
		"background": "#8be9fd",
		"foreground": "#282a36",
		"icon": "check",
		"iconColour": "#8be9fd",
		"type": "tag"
	},
	"todo-tree.excludeGlobs": [
		"**/node_modules/**"
	],
	"todo-tree.regex": "(($TAGS)|^\\s*- \\[ \\])",
	"todo-tree.showBadges": false,
	"todo-tree.statusBar": "tags",
	"todo-tree.tags": [
		"FIXME:",
		"TODO:"
	],
	"todo-tree.tagsOnly": true,
	"typescript.suggest.autoImports": false,
	"typescript.tsdk": "/home/dsifford/.local/share/npm/lib/node_modules/typescript/lib",
	"typescript.tsserver.log": "verbose",
	"uncrustify.configPath": "/home/dsifford/.uncrustify.cfg",
	"vim.easymotionMarkerBackgroundColor": "#ff5555",
	"vim.easymotionMarkerFontFamily": "Monaco",
	"vim.easymotionMarkerFontWeight": "bold",
	"vim.easymotionMarkerHeight": 20,
	"vim.easymotionMarkerWidthPerChar": 10,
	"vim.enableNeovim": true,
	"vim.foldfix": true,
	"vim.highlightedyank.color": "#ffb86c",
	"vim.highlightedyank.duration": 1000,
	"vim.highlightedyank.enable": true,
	"vim.leader": "<space>",
	"vim.neovimPath": "/usr/bin/nvim",
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": [
				";"
			],
			"after": [
				":"
			]
		},
		{
			"before": [
				":"
			],
			"commands": [
				"workbench.action.showCommands"
			]
		},
		{
			"before": [
				"K"
			],
			"commands": [
				"editor.action.showHover"
			]
		},
		{
			"before": [
				"g",
				"r"
			],
			"commands": [
				"references-view.find"
			]
		},
		{
			"before": [
				"<leader>",
				"f"
			],
			"commands": [
				"editor.action.formatDocument"
			]
		},
		{
			"before": [
				"<leader>",
				"z",
				"f"
			],
			"commands": [
				"editor.foldAllMarkerRegions"
			]
		},
		{
			"before": [
				"<leader>",
				"b",
				"l"
			],
			"commands": [
				"gitlens.toggleLineBlame"
			]
		},
		{
			"before": [
				"<leader>",
				"b",
				"f"
			],
			"commands": [
				"gitlens.toggleFileBlame"
			]
		},
		{
			"before": [
				"]",
				"c"
			],
			"commands": [
				"workbench.action.editor.nextChange"
			]
		},
		{
			"before": [
				"[",
				"c"
			],
			"commands": [
				"workbench.action.editor.previousChange"
			]
		}
	],
	"vim.searchHighlightColor": "rgba(80, 250, 123, 0.6)",
	"vim.sneak": true,
	"vim.sneakUseIgnorecaseAndSmartcase": true,
	"vim.useSystemClipboard": true,
	"vim.visualstar": true,
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"window.menuBarVisibility": "toggle",
	"window.zoomLevel": 0,
	"workbench.activityBar.visible": true,
	"workbench.colorCustomizations": {
		"editorBracketMatch.background": "#FF79C680",
		"editorBracketMatch.border": "#ff00ff"
	},
	"workbench.colorTheme": "Dracula",
	"workbench.sideBar.location": "left",
	"workbench.startupEditor": "none",
	"workbench.statusBar.visible": true,
	"workbench.tree.indent": 20
}
