
# Files
data.location=~/.local/share/task

# Turn off nag
nag=

# Adjust default verbosity
verbose=blank,label,footnote,new-id,affected,edit,special,project,sync,unwait

# Priority Settings
uda.priority.label=Priority
uda.priority.type=string
uda.priority.values=H,M,,L

urgency.uda.priority.H.coefficient=6.0
urgency.uda.priority.M.coefficient=3.9
urgency.uda.priority.L.coefficient=-1.0

# Colors
color=on
color.tagged=

color.label=white bold
color.label.sort=white bold

color.uda.priority.H=red bold
color.uda.priority.L=gray9
color.uda.priority.M=yellow

# Report Settings
report.next.columns=id,start.age,entry.age,depends,priority,project,tags,recur,scheduled.countdown,due.remaining,until.remaining,description.combined,urgency
report.next.sort=urgency-

# Sync settings
